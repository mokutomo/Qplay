<div class="container">
	<% @users.each do |user| %>
	<div class = "user">
		<div class="row">
			<div class="col-sm-3"><%= user.name %></div>

			<div class="col-sm-3"><%= user.email %></div>
			<div class="col-sm-3"><%= link_to "削除", admin_path(user.id), method: :delete,class: "btn btn-primary" %>
			</div>
		</div>
	</div>
	<% end %>
